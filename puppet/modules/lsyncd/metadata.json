{
  "name": "olevole-lsyncd",
  "version": "0.1.0",
  "author": "Oleg Ginzburg",
  "summary": "Yet another lsyncd module",
  "license": "BSD",
  "source": "https://github.com/olevole/puppet-lsyncd",
  "project_page": "https://forge.puppetlabs.com/olevole/lsyncd",
